//Recursion
class Solution {
public:
    void solve(string s, string t,int &i,int &j)
    {
        if(j>=s.length() || i>=t.length()){
            return;
        }
        if(s[j]==t[i]){
            j++;
        }
        i++;
        solve(s,t,i,j);
    }
    bool isSubsequence(string s, string t) {
        int i=0,j=0;
        solve(s,t,i,j);
        cout<<j;
        return j==s.length();
    }
};
